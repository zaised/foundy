({ds: 1});\n                        if (clearedBarricade) await clearedBarricade.update({hidden: !clearedBarricade.data.hidden});\n                    },\n                    icon: `<i class=\"fas fa-volume-up\"></i>`\n                }\n                \n            }\n        }).render(true);\n    }  else { // If the door is open \n        if (clearedBarricade) await clearedBarricade.update({hidden: !clearedBarricade.data.hidden});\n        if (door) await door.update({ds: 0});\n        playlist.stopAll();\n    }\n}\n\nawait clearBarricade();","folder":"6uvlXGlICsQdIAVF","sort":0,"flags":{"core":{"sourceId":"Macro.1A46fJUNGZAdKMG6"},"exportSource":{"world":"pf2e-beginner-box","system":"pf2e","coreVersion":"9.268","systemVersion":"3.8.4.11118"},"pf2e-beginner-box":{"isBB":true}},"ownership":{"default":0,"ZJ6r3OyrumO4W34r":3,"UkpZUJ3OLqgZ7Vnf":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"iPvtf4ZGr6qX8OnF","name":"Area 14- Cinder Rat","type":"script","author":"UkpZUJ3OLqgZ7Vnf","img":"icons/magic/fire/orb-lightning-sun.webp","scope":"global","command":"async function surpriseRat() {\n    const rat = canvas.scene.tokens.getName(\"Cinder Rat\");\n    const ratPlaylist = game.playlists.getName(\"The Cinder Rat\");\n    const ratSound = ratPlaylist.sounds.getName(\"The Rat's On Fire\");\n    const orbPlaylist = game.playlists.getName(\"Barricades\");\n    const orbSound = orbPlaylist.sounds.getName(\"Breaking The Stone Wall\");\n\n    if (canvas.scene.id === \"kiSlv7vcx4HrVCmd\") { // Original Floor\n        const orbLight = canvas.scene.lights.get(\"JpkPjTmT6VeJlC3d\");\n        const sleep = 250;\n\n        if (!orbLight.data.hidden) {\n            if (ratSound) ratPlaylist.playSound(ratSound);\n            if (orbSound) orbPlaylist.playSound(orbSound);\n            await orbLight.update({hidden: !orbLight.data.hidden});\n            await new Promise(r => setTimeout(r, sleep));\n            await rat.update({hidden: !rat.data.hidden});\n        } else {\n            await orbLight.update({hidden: !orbLight.data.hidden})\n            await rat.update({hidden: !rat.data.hidden});\n            ratPlaylist.stopAll();\n            orbPlaylist.stopAll();\n        }\n\n    } else if (canvas.scene.id === \"cgv9iVmx3dNIL3YA\")  { // Enhanced version\n        const orbLight = canvas.scene.lights.get(\"ryPdrDNEW7RPXXY9\");\n        const orbSecondaryLight = canvas.scene.lights.get(\"JpkPjTmT6VeJlC3d\");\n        const orbTile = canvas.scene.tiles.get(\"RNF4rcwejCd464Lh\");\n        const orbShatteredTile = canvas.scene.tiles.get(\"gaBF4dI05DdEVVRq\");\n\n        const sleep = 250;\n        if (!orbLight.data.hidden) {\n            if (ratSound) ratPlaylist.playSound(ratSound);\n            if (orbSound) orbPlaylist.playSound(orbSound);\n            await orbLight.update({hidden: !orbLight.data.hidden});\n            await orbSecondaryLight.update({hidden: !orbSecondaryLight.data.hidden});\n            await orbTile.update({hidden: !orbTile.data.hidden});\n            await orbShatteredTile.update({hidden: !orbShatteredTile.data.hidden});\n            await new Promise(r => setTimeout(r, sleep));\n            await rat.update({hidden: !rat.data.hidden});\n        } else {\n            await orbLight.update({hidden: !orbLight.data.hidden})\n            await orbSecondaryLight.update({hidden: !orbSecondaryLight.data.hidden});\n            await rat.update({hidden: !rat.data.hidden});\n            await orbTile.update({hidden: !orbTile.data.hidden});\n            await orbShatteredTile.update({hidden: !orbShatteredTile.data.hidden});\n            ratPlaylist.stopAll();\n            orbPlaylist.stopAll();\n        }\n    }\n}\n\nawait surpriseRat();","folder":"6uvlXGlICsQdIAVF","sort":100000,"flags":{"pf2e-beginner-box":{"isBB":true},"exportSource":{"world":"pf2e-beginner-box","system":"pf2e","coreVersion":"9.268","systemVersion":"3.8.4.11118"}},"owne